<% if controller.showAttachments %>
  <% other_formats_links do |f| %>
    <%= f.link_to('PDF', :url => {controller: controller.controller_name, action: 'export', startday: @startday, user_id: params[:user_id], format: 'pdf'}) %>
    <%= f.link_to('CSV', :url => {controller: controller.controller_name, action: 'export', startday: @startday, user_id: params[:user_id], format: 'csv'}) %>
  <% end %>
<% end %>

<% html_title l(:label_spent_time), l(:label_details) %>

<% content_for :header_tags do %>
    <%= auto_discovery_link_tag(:atom, {:issue_id => @issue, :format => 'atom', :key => User.current.rss_key}, :title => l(:label_spent_time)) %>
<% end %>

